---
import ServiceNavbarV1 from "@components/shared/MarketingPagesComponents/ServiceNavbarV1/ServiceNavbarV1.astro";
import BannerSectionV2 from "@components/shared/MarketingPagesComponents/BannerSectionV2/BannerSectionV2.astro";
import BookAppointmentSection from "@components/shared/MarketingPagesComponents/BookAppointmentSectionV1/BookAppointmentSection.astro";
import ContactUsSectionV1 from "@components/shared/MarketingPagesComponents/ContactUsSectionV1/ContactSectionV1.astro";
import TestimonialSectionV1 from "@components/shared/MarketingPagesComponents/TestimonialSectionV1/TestimonialSectionV1.astro";
import type { IMetaNewPageProps } from "@components/pages/PagePropsInterface/page_props_interface";
import { reduceFAQs } from "@utils/global_utils";
import AboutSection from "@components/shared/MarketingPagesComponents/AboutSection/AboutSection.astro";
import StaticCityClinics from "@components/shared/MarketingPagesComponents/StaticCityClinics/StaticCityClinics.astro";
import VeticServices from "@components/shared/MarketingPagesComponents/VeticServices/VeticServices.astro";
import SingleStaticCityClinics from "@components/shared/MarketingPagesComponents/SingleStaticCityClinics/SingleStaticCityClinics.astro";

const { content, srNumber, location } = Astro.props as IMetaNewPageProps;
const reducedFAQs = reduceFAQs(
  content?.Faq?.data ?? content?.Faq?.FAQ,
  "title",
  "content"
);
// console.log(isCars24Page, "Check cars24 URL");
const currentPath = Astro.url.pathname;
const isAdsureUrl = currentPath.includes("adsure");
---

<ServiceNavbarV1 location={location} srNumber={srNumber} />
<BannerSectionV2 content={content} />
<BookAppointmentSection content={content} location={location} />
<TestimonialSectionV1 content={content} />
<AboutSection content={content} />
<VeticServices content={content} />
{
  isAdsureUrl ? (
    <SingleStaticCityClinics content={content} />
  ) : (
    <StaticCityClinics content={content} />
  )
}
<ContactUsSectionV1
  title="Hello"
  location={location}
  content={content}
  srNumber={srNumber}
/>
